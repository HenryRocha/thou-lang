maths multiply(maths a, maths b)
{
    return_to_the_one_whom_bid_thou a * b;
}

maths main()
{
    maths x = sum(25, 26);
    thou_shalt_utter(x);

    maths y = multiply(10, 10);
    thou_shalt_utter(y);

    maths z = sum(10, multiply(100, 100));
    thou_shalt_utter(z);

    return_to_the_one_whom_bid_thou 0;
}

maths sum(maths a, maths b)
{
    return_to_the_one_whom_bid_thou a + b;
}
